{
  "version": 2,
  "builds": [
    {
      "src": "apps/html/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "apps/html/blog/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "apps/html/jak-zostac-programista-2025/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "apps/html/author/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/blog/(.*)",
      "dest": "/apps/html/blog/$1"
    },
    {
      "src": "/jak-zostac-programista-2025/(.*)",
      "dest": "/apps/html/jak-zostac-programista-2025/$1"
    },
    {
      "src": "/author/(.*)",
      "dest": "/apps/html/author/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/html/$1"
    }
  ],
